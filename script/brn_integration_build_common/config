[default]
devices=littleton300_256M 
modules=android rootfs kernel bootloader factory update release

[littleton300_256M]
TARGET_PRODUCT=littleton
cpu_type=pxa300
FLASH_SIZE=256M
kernel=linux-2.6.28.froyo
kernel_config=android_froyo_an0001_pxa300_release_defconfig
recovery_kernel_config=android_froyo_an0001_pxa300_recovery_defconfig
device_path=vendor/marvell/littleton
bootloader=blob-wtptp
bootloader_makefile=Makefile.256M
customers=default default-preinstall
release_notes=ReleaseNote_A1.txt
branch=a1

